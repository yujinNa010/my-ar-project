{
  "version": 2,
  "cleanUrls": true,
  "trailingSlash": false,
  "headers": [
    {
      "source": "/(.*)\\.glb",
      "headers": [
        { "key": "Content-Type", "value": "model/gltf-binary" }
      ]
    },
    {
      "source": "/(.*)\\.usdz",
      "headers": [
        { "key": "Content-Type", "value": "model/vnd.usdz+zip" }
      ]
    }
  ]
}
